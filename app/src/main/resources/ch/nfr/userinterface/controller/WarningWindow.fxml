<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="30.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ch.nfr.userinterface.controller.WarningController">
   <children>
      <Label fx:id="warningMessage" alignment="CENTER" minHeight="17.6" minWidth="50.0" textAlignment="JUSTIFY" />
      <HBox alignment="CENTER" spacing="10.0" VBox.vgrow="NEVER">
         <children>
            <Button mnemonicParsing="false" onAction="#cancelOperation" prefWidth="80.0" text="Abbrechen" />
            <Button mnemonicParsing="false" onAction="#acceptOperation" prefWidth="80.0" text="Akzeptieren" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
